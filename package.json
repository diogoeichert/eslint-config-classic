		"lint",
		"linter",
		"linting",
		"padding",
		"perfect",
		"pretty",
		"quotes",
		"readability",
		"rules",
		"semicolons",
		"standard",
		"tabs"
	],
	"author": "Diogo Eichert",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/diogoeichert/eslint-config-classic/issues"
	},
	"funding": {
		"url": "https://github.com/sponsors/diogoeichert"
	},
	"homepage": "http://diogoeichert.github.io/eslint-config-classic/",
