{
	"name": "eslint-config-classic",
	"version": "1.6.0",
	"description": "ESlint rules for classic-style JavaScript",
	"main": "index.js",
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": false,
		"useTabs": true
	},
	"scripts": {
		"lint": "eslint --fix .",
		"prepublishOnly": "git checkout main && git pull --rebase && npm test && git push && git push --tags",
		"test": "eslint ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/diogoeichert/eslint-config-classic.git"
	},
	"keywords": [
		"classic",
		"config",
		"eslint",
		"classic",
		"rules",
		"standard"
	],
	"author": "Diogo Eichert",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/diogoeichert/eslint-config-classic/issues"
	},
	"funding": {
		"url": "https://www.paypal.com/cgi-bin/webscr?cmd=_donations&business=diogoeichert%40icloud.com&item_name=eslint-config-classic&currency_code=EUR"
	},
	"homepage": "http://diogoeichert.github.io/eslint-config-classic/",
	"dependencies": {
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.0"
	}
}
